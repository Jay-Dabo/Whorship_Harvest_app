<% @page_title = "Resources" %>
<div class="header">
  <%= render(:partial => 'layouts/navbar')%>
</div>
<p id="notice"><%= notice %></p>

<h1>Listing Resources</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Published</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @resources.each do |resource| %>
    <tr>
      <td><%= resource.title %></td>
      <td><%= resource.description %></td>
      <td><%= resource.published %></td>
      <td><%= link_to 'Show', resource %></td>
      <td><%= link_to 'Edit', edit_resource_path(resource) %></td>
      <td><%= link_to 'Destroy', resource, method: :delete, data: { confirm: 'Are you sure?' } %></td>
    </tr>
    <% end %>
  </tbody>
</table>

<br>
<table class="table table-striped table-hover ">
  <tbody>
    <tr>
      <td>
        
      <%=image_tag('music2.jpg')%>
      </td>
      <td>
        <img src="audio-and-video2.jpg" data-src="holder.js/200x200" class="img-thumbnail" alt="200x200">
      </td>
    </tr>

 <tr>
      <td>
        <img src="vidcam.jpg" data-src="holder.js/200x200" class="img-thumbnail" alt="200x200">
      </td>
      <td>
        <img src="data:image/png;base64," style="width: 200px; height: 200px;" data-src="holder.js/200x200" class="img-thumbnail" alt="200x200">
      </td>
    </tr>

  </tbody>
</table>

<%= link_to 'New Resource', new_resource_path %>
</div>
<div class="footer">
  <%= render(:partial => 'layouts/footer')%>
</div>